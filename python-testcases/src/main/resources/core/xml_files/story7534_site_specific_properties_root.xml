            <bootable>true</bootable>
            <disk_part>false<!--note: this property is not updatable--></disk_part>
            <name>sda</name>
            <size>40G</size>
            <uuid>%%DISK1_UUID%%</uuid>
          </litp:disk>
        </litp:system-disks-collection>
        <litp:bmc id="bmc">
          <ipaddress>%%bmc_IP%%</ipaddress>
          <password_key>key-for-user</password_key>
          <username>no-user</username>
        </litp:bmc>
      </litp:blade>
      <litp:blade id="sys3">
        <system_name>MN2</system_name>
        <litp:system-controllers-collection id="controllers"/>
        <litp:system-disks-collection id="disks">
          <litp:disk id="disk0">
            <bootable>true</bootable>
            <disk_part>false<!--note: this property is not updatable--></disk_part>
            <name>sda</name>
            <size>40G</size>
            <uuid>kgb</uuid>
          </litp:disk>
        </litp:system-disks-collection>
        <litp:bmc id="bmc">
          <ipaddress>192.168.0.44</ipaddress>
          <password_key>key-for-user</password_key>
          <username>no-user</username>
        </litp:bmc>
      </litp:blade>
    </litp:infrastructure-systems-collection>
  </litp:infrastructure>
  <litp:ms id="ms">
    <hostname>%%ms_node%%</hostname>
    <litp:ms-configs-collection id="configs">
      <litp:alias-node-config id="alias_config">
        <litp:alias-node-config-aliases-collection id="aliases">
          <litp:alias id="ntp_alias1">
            <address>%%alias_IP%%</address>
            <alias_names>%%alias_name%%</alias_names>
          </litp:alias>
        </litp:alias-node-config-aliases-collection>
      </litp:alias-node-config>
      <litp:dns-client id="dns_client">
        <search>ammeonvpn.com,exampleone.com,exampletwo.com,examplethree.com,examplefour.com,examplefive.com</search>
        <litp:dns-client-nameservers-collection id="nameservers">
          <litp:nameserver id="init_name_server">
            <ipaddress>%%IP_01%%</ipaddress>
            <position>1</position>
          </litp:nameserver>
        </litp:dns-client-nameservers-collection>
      </litp:dns-client>
      <litp:firewall-node-config id="fw_config_init">
        <drop_all>true</drop_all>
        <litp:firewall-node-config-rules-collection id="rules">
          <litp:firewall-rule id="fw_icmp">
            <action>accept</action>
            <name>100 icmp</name>
            <proto>icmp</proto>
          </litp:firewall-rule>
          <litp:firewall-rule id="fw_icmpv6">
            <action>accept</action>
            <name>101 icmpv6</name>
            <proto>ipv6-icmp</proto>
            <provider>ip6tables</provider>
          </litp:firewall-rule>
          <litp:firewall-rule id="fw_nfstcp">
            <action>accept</action>
            <dport>111,2049,4001</dport>
            <name>001 nfstcp</name>
            <proto>tcp</proto>
            <state>NEW</state>
          </litp:firewall-rule>
          <litp:firewall-rule id="fw_nfsudp">
            <action>accept</action>
            <dport>111,2049,4001</dport>
            <name>011 nfsudp</name>
            <proto>udp</proto>
            <state>NEW</state>
          </litp:firewall-rule>
        </litp:firewall-node-config-rules-collection>
      </litp:firewall-node-config>
      <litp:sysparam-node-config id="mynodesysctl">
        <litp:sysparam-node-config-params-collection id="params">
          <litp:sysparam id="sysctl_MS01">
            <key>net.ipv4.udp_mem</key>
            <value>24794401 33059201 49588801</value>
          </litp:sysparam>
        </litp:sysparam-node-config-params-collection>
      </litp:sysparam-node-config>
    </litp:ms-configs-collection>
    <litp:ms-file_systems-collection id="file_systems"/>
    <litp:ms-items-collection id="items">
      <litp:ntp-service-inherit source_path="/software/items/ntp1" id="ntp">
        <litp:ntp-service-servers-collection-inherit source_path="/software/items/ntp1/servers" id="servers">
          <litp:ntp-server-inherit source_path="/software/items/ntp1/servers/server0" id="server0"/>
        </litp:ntp-service-servers-collection-inherit>
      </litp:ntp-service-inherit>
    </litp:ms-items-collection>
    <litp:ms-network_interfaces-collection id="network_interfaces">
      <litp:bond id="b0">
        <ipaddress>%%bond_01%%</ipaddress>
        <ipv6address>2001:1b70:82a1:0103::42/64</ipv6address>
        <network_name>mgmt</network_name>
        <device_name>%%MGM_BOND%%<!--note: this property is not updatable--></device_name>
        <miimon>100</miimon>
        <mode>1</mode>
      </litp:bond>
      <litp:eth id="if0">
        <device_name>eth0<!--note: this property is not updatable--></device_name>
        <macaddress>00:50:56:00:00:42<!--note: this property is not updatable--></macaddress>
        <master>bondmgmt</master>
      </litp:eth>
      <litp:eth id="if1">
        <device_name>eth1<!--note: this property is not updatable--></device_name>
        <macaddress>00:50:56:00:00:60<!--note: this property is not updatable--></macaddress>
        <master>bondmgmt</master>
      </litp:eth>
    </litp:ms-network_interfaces-collection>
    <litp:ms-routes-collection id="routes">
      <litp:route-inherit source_path="/infrastructure/networking/routes/r1" id="r1"/>
      <litp:route6-inherit source_path="/infrastructure/networking/routes/default_ipv6" id="r2_ipv6"/>
    </litp:ms-routes-collection>
    <litp:ms-services-collection id="services">
      <litp:cobbler-service id="cobbler">
        <authentication>authn_configfile</authentication>
        <ksm_ksname>litp.ks</ksm_ksname>
        <ksm_path>/var/lib/cobbler/kickstarts</ksm_path>
        <ksm_selinux_mode>enforcing</ksm_selinux_mode>
        <manage_dhcp>true</manage_dhcp>
        <manage_dns>false</manage_dns>
        <puppet_auto_setup>true</puppet_auto_setup>
        <remove_old_puppet_certs_automatically>true</remove_old_puppet_certs_automatically>
        <rsync_disabled>false</rsync_disabled>
        <sign_puppet_certs_automatically>true</sign_puppet_certs_automatically>
      </litp:cobbler-service>
    </litp:ms-services-collection>
    <litp:blade-inherit source_path="/infrastructure/systems/sys1" id="system">
      <litp:system-controllers-collection-inherit source_path="/infrastructure/systems/sys1/controllers" id="controllers"/>
      <litp:system-disks-collection-inherit source_path="/infrastructure/systems/sys1/disks" id="disks"/>
    </litp:blade-inherit>
  </litp:ms>
  <litp:root-plans-collection id="plans"/>
  <litp:software id="software">
    <litp:software-deployables-collection id="deployables"/>
    <litp:software-images-collection id="images"/>
    <litp:software-items-collection id="items">
      <litp:package id="cups">
        <epoch>0</epoch>
        <name>cups</name>
      </litp:package>
      <litp:ntp-service id="ntp1">
        <litp:ntp-service-servers-collection id="servers">
          <litp:ntp-server id="server0">
            <server>ntpAlias1</server>
          </litp:ntp-server>
        </litp:ntp-service-servers-collection>
      </litp:ntp-service>
      <litp:package id="ricci">
        <epoch>0</epoch>
        <name>ricci</name>
      </litp:package>
    </litp:software-items-collection>
    <litp:software-profiles-collection id="profiles">
      <litp:os-profile id="os_prof1">
        <arch>x86_64</arch>
        <breed>redhat</breed>
        <kopts_post>console=ttyS0,115200</kopts_post>
        <name>os-profile1</name>
        <path>/var/www/html/7/os/x86_64/</path>
        <version>rhel7</version>
      </litp:os-profile>
    </litp:software-profiles-collection>
    <litp:software-runtimes-collection id="runtimes"/>
    <litp:software-services-collection id="services">
      <litp:service id="ricci">
        <cleanup_command>/bin/true</cleanup_command>
        <service_name>ricci</service_name>
        <litp:service-packages-collection id="packages">
          <litp:package-inherit source_path="/software/items/ricci" id="pkg1"/>
        </litp:service-packages-collection>
      </litp:service>
    </litp:software-services-collection>
  </litp:software>
</litp:root>
